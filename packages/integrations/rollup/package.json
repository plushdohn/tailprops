{
  "name": "rollup-plugin-tailprops",
  "version": "0.2.0",
  "description": "Rollup plugin for Tailprops",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm build:commonjs && pnpm build:esm",
    "build:commonjs": "rimraf dist/cjs && tsc --module commonjs --outDir dist/cjs",
    "build:esm": "rimraf dist/esm && tsc --module esnext --outDir dist/esm",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "tailprops": "workspace:^0.2.0"
  },
  "peerDependencies": {
    "rollup": "^3.15.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "jest": "^29.4.2",
    "rollup": "^3.15.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
