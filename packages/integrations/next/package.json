{
  "name": "tailprops-next",
  "version": "0.2.1",
  "description": "Utility function for quickly including Tailprops in a Next.js project",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm build:commonjs && pnpm build:esm",
    "build:commonjs": "rimraf dist/cjs && tsc --module commonjs --outDir dist/cjs",
    "build:esm": "rimraf dist/esm && tsc --module esnext --outDir dist/esm",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "tailprops-webpack-plugin": "^0.2.2"
  },
  "peerDependencies": {
    "next": "^13.1.6"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "jest": "^29.4.2",
    "next": "13.1.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
