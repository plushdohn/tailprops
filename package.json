{
  "name": "tailprops",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/**",
    "packages/**"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
